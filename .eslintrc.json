{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "consistent-return": ["error", { "treatUndefinedAsUnspecified": false }],
    "no-underscore-dangle": "off",
    "no-tabs": "off",
    "indent": "off",
    "default-case": "off",
    "no-lone-blocks": "off",
    "no-return-await" : "off",
    "no-else-return": "off"
  }
}